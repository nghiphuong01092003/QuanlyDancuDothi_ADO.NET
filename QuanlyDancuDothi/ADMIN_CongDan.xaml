<Window x:Class="QuanlyDancuDothi.ADMIN_CongDan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:QuanlyDancuDothi"
        mc:Ignorable="d"
        Title="ADMIN_CongDan" Height="750" Width="1140"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{x:Null}"
        AllowsTransparency="True"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="BoolToVis"/>
    </Window.Resources>

    <materialDesign:Card Background="{DynamicResource MaterialDesignPaper}" UniformCornerRadius="15" Margin="25" materialDesign:ElevationAssist.Elevation="Dp4">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="190"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Border Grid.Column="0" Height="700" CornerRadius="15 0 0 15" Background="{DynamicResource MaterialDesignLightSeparatorBackground}">
                <StackPanel Margin="15">
                    <WrapPanel Margin="0 10 5 60" HorizontalAlignment="Center">
                        <Image RenderOptions.BitmapScalingMode="HighQuality" Source="/Images/copy_logo_MFIT.png" Width="30" />
                        <TextBlock Text="ADMIN" FontSize="20" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0"/>
                    </WrapPanel>
                    <Button x:Name="btn_TrangChu" Style="{StaticResource MaterialDesignFlatButton}" materialDesign:ButtonAssist.CornerRadius="6" Width="auto"
                            Height="35" materialDesign:RippleAssist.Feedback="White" Margin="0 0 0 15" Click="btn_TrangChu_Click">
                        <WrapPanel Width="120">
                            <materialDesign:PackIcon Kind="Home" VerticalAlignment="Center" Margin="0 0 20 0"/>
                            <TextBlock Text="Trang chủ"/>
                        </WrapPanel>
                    </Button>
                    <Button x:Name="btn_CongDan" Style="{StaticResource MaterialDesignFlatButton}" materialDesign:ButtonAssist.CornerRadius="6" Width="auto"
                            Height="35" materialDesign:RippleAssist.Feedback="White" Margin="0 0 0 15">
                        <WrapPanel Width="120">
                            <materialDesign:PackIcon Kind="People" VerticalAlignment="Center" Margin="0 0 20 0"/>
                            <TextBlock Text="Công dân"/>
                        </WrapPanel>
                    </Button>
                    <Button x:Name="btn_QuanLyDon" Style="{StaticResource MaterialDesignFlatButton}" materialDesign:ButtonAssist.CornerRadius="6" Width="auto"
                            Height="35" materialDesign:RippleAssist.Feedback="White" Margin="0 0 0 15" Click="btn_QuanLyDon_Click">
                        <WrapPanel Width="120">
                            <materialDesign:PackIcon Kind="FolderMedia" VerticalAlignment="Center" Margin="0 0 20 0"/>
                            <TextBlock Text="Quản lý đơn"/>
                        </WrapPanel>
                    </Button>
                    <Button x:Name="btn_danhgia" Style="{StaticResource MaterialDesignFlatButton}" materialDesign:ButtonAssist.CornerRadius="6" Width="auto"
                            Height="35" materialDesign:RippleAssist.Feedback="White" Margin="0 0 0 15" Click="btn_danhgia_Click">
                        <WrapPanel Width="120">
                            <materialDesign:PackIcon Kind="RateReview" VerticalAlignment="Center" Margin="0 0 20 0"/>
                            <TextBlock Text="Đánh giá"/>
                        </WrapPanel>
                    </Button>

                    <StackPanel Orientation="Horizontal" Margin="10 0 10 15" VerticalAlignment="Center" Width="auto" Height="35">
                        <materialDesign:PackIcon Kind="MoonAndStars" VerticalAlignment="Center" Margin="0 0 10 0"/>
                        <TextBlock VerticalAlignment="Center" Text="Chế độ tối" FontWeight="SemiBold"/>
                        <ToggleButton Cursor="Hand" ToolTip="Bật" Margin="12 0 8 0"
                                      x:Name="themeToggle" IsChecked="{Binding IsDarkTheme}"
                                      Click="toggleTheme">
                        </ToggleButton>
                    </StackPanel>

                    <materialDesign:ColorZone Mode="Light" Background="AliceBlue" CornerRadius="15" Padding="25 0" Margin="0 170 0 0">
                        <StackPanel>
                            <Image RenderOptions.BitmapScalingMode="HighQuality" Source="/Images/copy_logo_MFIT.png" Margin="0 -60 0 0" Height="100" Width="100" HorizontalAlignment="Center"/>
                            <TextBlock Foreground="DodgerBlue" FontSize="11" FontWeight="SemiBold" TextAlignment="Center"> Nâng cấp  <Bold FontWeight="Heavy">PRO</Bold> để <LineBreak/>trải nghiệm tốt hơn</TextBlock>
                            <Button Style="{StaticResource MaterialDesignFlatMidBgButton}" materialDesign:ElevationAssist.Elevation="Dp0" Height="28" materialDesign:ButtonAssist.CornerRadius="6"
                                    FontSize="11" Content="Nâng cấp" Margin="0 20 0 25"/>
                        </StackPanel>
                    </materialDesign:ColorZone>
                </StackPanel>
            </Border>
            <Grid Grid.Column="1" Margin="30 12 0 0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                </Grid.RowDefinitions>
                <DockPanel Grid.Row="0" Margin="0 12 5 0">
                    <Button x:Name="btn_Exit" Style="{StaticResource MaterialDesignIconButton}" materialDesign:RippleAssist.Feedback="LightPink" Foreground="LightCoral" materialDesign:ButtonAssist.CornerRadius="6" Height="40"
                            Width="40" DockPanel.Dock="Right" HorizontalAlignment="Right" Click="btn_Exit_Click">
                        <materialDesign:PackIcon Kind="CloseBoxesOutline"/>
                    </Button>
                    <Button x:Name="btn_LogOut" Style="{StaticResource MaterialDesignIconButton}" materialDesign:RippleAssist.Feedback="LightPink" Foreground="LightCoral" materialDesign:ButtonAssist.CornerRadius="6" Height="40"
                            Width="40" DockPanel.Dock="Right" HorizontalAlignment="Right" Click="btn_LogOut_Click">
                        <materialDesign:PackIcon Kind="LogoutVariant"/>
                    </Button>
                </DockPanel>
                <TextBlock Grid.Row="1" Text="Tra cứu thông tin công dân" FontSize="32" FontWeight="SemiBold" Margin="20 0 0 0"/>
                <TabControl Grid.Row="2" IsSynchronizedWithCurrentItem="True" Style="{StaticResource MaterialDesignUniformTabControl}" Margin="0 5 10 5">
                    <TabControl.Template>
                        <ControlTemplate TargetType="{x:Type TabControl}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <ScrollViewer HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Disabled" Style="{StaticResource MaterialDesignScrollViewer}">
                                    <TabPanel x:Name="HeaderPanel" IsItemsHost="True" Margin="0,4,0,0"/>
                                </ScrollViewer>
                                <ContentPresenter x:Name="PART_SelectedContentHost" Margin="4" ContentSource="SelectedContent" Grid.Row="1"/>
                            </Grid>
                        </ControlTemplate>
                    </TabControl.Template>
                    <TabItem x:Name="tab_tt" Header="Thông tin công dân" Width="auto" Height="45">
                        <Grid Margin="0 5 10 5">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="auto"/>
                            </Grid.RowDefinitions>
                            <StackPanel Grid.Row="0" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0 10 20 10" Width="auto" Height="auto" Orientation="Horizontal">
                                <TextBox x:Name="txt_tt_keyword" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="150" Background="Transparent" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0 0 10 0"
                                         materialDesign:HintAssist.Hint="Thông tin cần tìm" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" KeyDown="txt_tt_keyword_KeyDown"/>
                                <Button x:Name="btn_TraCuu_ThongTin" Style="{StaticResource MaterialDesignFlatAccentButton}" materialDesign:ButtonAssist.CornerRadius="7" materialDesign:RippleAssist.Feedback="Salmon" Margin="0 0 10 0"
                                    Foreground="{DynamicResource PrimaryHueDarkBrush}" Width="100" Height="35" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="13" Click="btn_TraCuu_ThongTin_Click">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <materialDesign:PackIcon Kind="Magnify" VerticalAlignment="Center" Margin="0 0 5 0"/>
                                        <TextBlock Text="Tra cứu" VerticalAlignment="Center" FontSize="13" FontWeight="SemiBold"/>
                                    </StackPanel>
                                </Button>
                            </StackPanel>
                            <WrapPanel Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Stretch" Margin="0 5 0 0">
                                <TextBox x:Name="txt_tt_hoten" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="200" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Họ và tên" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" IsEnabled="False" />
                                <TextBox x:Name="txt_tt_gioitinh" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="100" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Giới tính" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" IsEnabled="False"/>
                                <TextBox x:Name="txt_tt_cccd" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="160" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="CCCD" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold"/>
                                <TextBox x:Name="txt_tt_ncCccd" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="100" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Nơi cấp CCCD" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold"/>
                                <DatePicker x:Name="dtp_tt_ngcCccd" Style="{StaticResource MaterialDesignOutlinedDatePicker}" Margin="5 0 5 10" materialDesign:HintAssist.Hint="Ngày cấp CCCD" Width="150"
                                            BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" Background="Transparent">
                                </DatePicker>
                                <DatePicker x:Name="dtp_tt_ngaysinh" Style="{StaticResource MaterialDesignOutlinedDatePicker}" Margin="5 0 5 10" materialDesign:HintAssist.Hint="Ngày sinh" Width="150"
                                            BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" Background="Transparent" IsTodayHighlighted="False" IsEnabled="False">
                                </DatePicker>
                                <TextBox x:Name="txt_tt_noisinh" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="100" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Nơi sinh" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" IsEnabled="False"/>
                                <TextBox x:Name="txt_tt_quoctich" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="95" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Quốc tịch" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" IsEnabled="False"/>
                                <TextBox x:Name="txt_tt_dantoc" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="100" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Dân tộc" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" IsEnabled="False"/>
                                <TextBox x:Name="txt_tt_quequan" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="100" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Quê quán" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" IsEnabled="False"/>
                                <TextBox x:Name="txt_tt_maks" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="90" Background="Transparent" Margin="5 0 5 10"
                                    materialDesign:HintAssist.Hint="Mã Khai sinh" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold"/>
                                <Button x:Name="btn_DienTT_Khaisinh" Style="{StaticResource MaterialDesignFlatButton}" Width="auto" Margin="5 0 5 10" Click="btn_DienTT_Khaisinh_Click">
                                    <materialDesign:PackIcon Kind="Upload"/>
                                </Button>
                                <TextBox x:Name="txt_tt_diachi" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="420" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Địa chỉ" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" IsEnabled="False"/>
                                <TextBox x:Name="txt_tt_sdt" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="313" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Số điện thoại" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold"/>
                                <TextBox x:Name="txt_tt_email" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="200" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Email" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold"/>
                                <TextBlock Text="Nhập lại" Margin="100 0 0 0" Height="24"/>
                                <Button x:Name="btn_clear_tt" Style="{StaticResource MaterialDesignFlatButton}" Width="auto" Margin="5 5 5 10" Click="btn_clear_tt_Click" >
                                    <materialDesign:PackIcon Kind="Delete"/>
                                </Button>
                            </WrapPanel>
                            <DataGrid CellStyle="{DynamicResource DataGridCellStyle1}" RowStyle="{DynamicResource DataGridRowStyle1}" Style="{StaticResource MaterialDesignDataGrid}" Grid.Row="1" x:Name="dtg_ThongTin" AutoGenerateColumns="False" CanUserAddRows="True" CanUserReorderColumns="False" GridLinesVisibility="None" SelectionChanged="dtg_ThongTin_SelectionChanged" Loaded="dtg_ThongTin_Loaded">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Binding="{Binding HoTen}" Width="150" Header="Họ và tên" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding GioiTinh}" Width="auto" Header="Giới tính" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding CCCD}" Width="auto" Header="CCCD" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding NcCccd}" Width="auto" Header="Nơi cấp CCCD" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding NgcCccd, StringFormat=d}" Width="auto" Header="Ngày cấp CCCD" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding NgaySinh, StringFormat=d}" Width="auto" Header="Ngày sinh" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding NoiSinh}" Width="auto" Header="Nơi sinh" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding QuocTich}" Width="auto" Header="Quốc tịch" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding DanToc}" Width="auto" Header="Dân tộc" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding QueQuan}" Width="auto" Header="Quê quán" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding DiaChi}" Width="auto" Header="Nơi ở hiện tại" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding SDT}" Width="auto" Header="Số điện thoại" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding Email}" Width="auto" Header="Email" IsReadOnly="True"/>
                                </DataGrid.Columns>
                            </DataGrid>
                            <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Left">
                                <Button x:Name="btn_Them_ThongTin" Style="{StaticResource MaterialDesignFlatAccentButton}" Grid.Row="1" materialDesign:ButtonAssist.CornerRadius="7" materialDesign:RippleAssist.Feedback="Salmon"
                                    Foreground="{DynamicResource PrimaryHueDarkBrush}" Width="100" Height="35" HorizontalAlignment="Right" Margin="0 10 20 0" FontSize="13" Click="btn_Them_ThongTin_Click">
                                    <StackPanel Orientation="Horizontal">
                                        <materialDesign:PackIcon Kind="Plus" VerticalAlignment="Center" Width="13" Height="13" Margin="0 1 8 0"/>
                                        <TextBlock Text="Thêm"/>

                                    </StackPanel>
                                </Button>
                                <Button x:Name="btn_Xoa_ThongTin" Style="{StaticResource MaterialDesignFlatAccentButton}" Grid.Row="1" materialDesign:ButtonAssist.CornerRadius="7" materialDesign:RippleAssist.Feedback="Salmon"
                                    Foreground="{DynamicResource PrimaryHueDarkBrush}" Width="100" Height="35" HorizontalAlignment="Right" Margin="0 10 0 0" FontSize="13" Click="btn_Xoa_ThongTin_Click">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <materialDesign:PackIcon Kind="Delete" VerticalAlignment="Center" Margin="0 0 5 0"/>
                                        <TextBlock Text="Xóa" VerticalAlignment="Center" FontSize="13" FontWeight="SemiBold"/>
                                    </StackPanel>
                                </Button>
                                <Button x:Name="btn_Sua_ThongTin1" Style="{StaticResource MaterialDesignFlatAccentButton}" Grid.Row="1" materialDesign:ButtonAssist.CornerRadius="7" materialDesign:RippleAssist.Feedback="Salmon"
                                    Foreground="{DynamicResource PrimaryHueDarkBrush}" Width="100" Height="35" HorizontalAlignment="Right" Margin="0 10 20 0" FontSize="13" Click="btn_Sua_ThongTin1_Click">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <materialDesign:PackIcon Kind="AutoFix" VerticalAlignment="Center" Margin="0 0 5 0"/>
                                        <TextBlock x:Name="btn_Sua_ThongTin" Text="Sửa" VerticalAlignment="Center" FontSize="13" FontWeight="SemiBold"/>
                                    </StackPanel>
                                </Button>
                            </StackPanel>
                        </Grid>
                    </TabItem>
                    <TabItem x:Name="tab_khaisinh" Header="Khai sinh" Width="auto" Height="45">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="auto"/>
                            </Grid.RowDefinitions>
                            <WrapPanel Grid.Row="0" HorizontalAlignment="Center" Margin="0 5 0 5" Width="840">
                                <TextBox x:Name="txt_khaisinh_maks" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="120" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Mã số khai sinh" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" IsEnabled="False" />

                                <TextBox x:Name="txt_khaisinh_hoten" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="200" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Họ và tên" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" />
                                <ComboBox x:Name="cmb_khaisinh_gioitinh" Background="Transparent" Height="auto" Width="100" materialDesign:HintAssist.Hint="Giới tính" BorderBrush="{DynamicResource MaterialDesignDivider}"
                                         FontSize="10" FontWeight="SemiBold" Margin="5 0 5 0"/>
                                <DatePicker x:Name="dtp_khaisinh_ngaysinh" Style="{StaticResource MaterialDesignOutlinedDatePicker}" Margin="5 0 5 10" materialDesign:HintAssist.Hint="Ngày sinh" Width="150"
                                            BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" Background="Transparent"/>
                                <ComboBox x:Name="cmb_khaisinh_noisinh" Background="Transparent" Height="auto" Width="130" materialDesign:HintAssist.Hint="Nơi sinh" BorderBrush="{DynamicResource MaterialDesignDivider}"
                                            FontSize="10" FontWeight="SemiBold" Margin="5 0 5 10"/>
                                <ComboBox x:Name="cmb_khaisinh_dantoc" Background="Transparent" Height="auto" Width="130" materialDesign:HintAssist.Hint="Dân tộc" BorderBrush="{DynamicResource MaterialDesignDivider}"
                                            FontSize="10" FontWeight="SemiBold" Margin="5 0 5 10"/>
                                <ComboBox x:Name="cmb_khaisinh_quoctich" Background="Transparent" Height="auto" Width="130" materialDesign:HintAssist.Hint="Quốc tịch" BorderBrush="{DynamicResource MaterialDesignDivider}"
                                            FontSize="10" FontWeight="SemiBold" Margin="5 0 5 10"/>
                                <ComboBox x:Name="cmb_khaisinh_quequan" Background="Transparent" Height="auto" Width="130" materialDesign:HintAssist.Hint="Quê quán" BorderBrush="{DynamicResource MaterialDesignDivider}"
                                            FontSize="10" FontWeight="SemiBold" Margin="5 0 5 10"/>
                                <TextBox x:Name="txt_khaisinh_cha" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="120" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Cha" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" LostFocus="txt_khaisinh_cha_LostFocus" />
                                <TextBox x:Name="txt_khaisinh_me" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="120" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Mẹ" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" LostFocus="txt_khaisinh_me_LostFocus" />
                                <TextBox x:Name="txt_khaisinh_ngkhaisinh" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="120" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Người khai sinh" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" LostFocus="txt_khaisinh_ngkhaisinh_LostFocus" />
                                <ComboBox x:Name="cmb_khaisinh_quanhe" Background="Transparent" Height="auto" Width="130" materialDesign:HintAssist.Hint="Quan hệ" BorderBrush="{DynamicResource MaterialDesignDivider}"
                                         FontSize="10" FontWeight="SemiBold" Margin="5 0 5 10"/>
                                <DatePicker x:Name="dtp_khaisinh_ngaydk" Style="{StaticResource MaterialDesignOutlinedDatePicker}" Margin="5 0 5 10" materialDesign:HintAssist.Hint="Ngày đăng ký" Width="150"
                                            BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" Background="Transparent"/>
                                <ComboBox x:Name="cmb_khaisinh_noidk" Background="Transparent" Height="auto" Width="200" materialDesign:HintAssist.Hint="Nơi đăng ký" BorderBrush="{DynamicResource MaterialDesignDivider}"
                                            FontSize="10" FontWeight="SemiBold" Margin="5 0 5 10"/>
                                <TextBlock Text="Nhập lại" Margin="100 0 0 0" Height="24"/>
                                <Button x:Name="btn_clear_ks" Style="{StaticResource MaterialDesignFlatButton}" Width="auto" Margin="5 5 5 10" Click="btn_clear_ks_Click" >
                                    <materialDesign:PackIcon Kind="Delete"/>
                                </Button>
                            </WrapPanel>
                            <DataGrid CellStyle="{DynamicResource DataGridCellStyle1}" RowStyle="{DynamicResource DataGridRowStyle1}" Grid.Row="1" x:Name="dtg_KhaiSinh" AutoGenerateColumns="False" CanUserAddRows="False" CanUserReorderColumns="False" GridLinesVisibility="None" Loaded="dtg_KhaiSinh_Loaded" SelectionChanged="dtg_KhaiSinh_SelectionChanged">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Binding="{Binding MaKS}" Width="auto" Header="Mã số khai sinh" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding HoTenKS}" Width="auto" Header="Họ và tên" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding GioiTinh}" Width="auto" Header="Giới tính" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding NgaySinh, StringFormat=d}" Width="auto" Header="Ngày sinh"/>
                                    <DataGridTextColumn Binding="{Binding NoiSinh}" Width="auto" Header="Nơi Sinh" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding DanToc}" Width="auto" Header="Dân tộc" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding QuocTich}" Width="auto" Header="Quốc tịch" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding QueQuan}" Width="auto" Header="Quê quán" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding Cha}" Width="auto" Header="Cha" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding Me}" Width="auto" Header="Mẹ" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding NguoiKhaiSinh}" Width="auto" Header="Người khai sinh" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding QuanHe}" Width="auto" Header="Quan hệ" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding NgayDk, StringFormat=d}" Width="auto" Header="Ngày đăng kí"/>
                                    <DataGridTextColumn Binding="{Binding NoiDk}" Width="auto" Header="Nơi đăng kí" IsReadOnly="True"/>
                                </DataGrid.Columns>
                            </DataGrid>
                            <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Left">
                                <Button x:Name="btn_Them_KhaiSinh" Style="{StaticResource MaterialDesignFlatAccentButton}" Grid.Row="1" materialDesign:ButtonAssist.CornerRadius="7" materialDesign:RippleAssist.Feedback="Salmon"
                                    Foreground="{DynamicResource PrimaryHueDarkBrush}" Width="100" Height="35" HorizontalAlignment="Right" Margin="0 10 20 0" FontSize="13" Click="btn_Them_KhaiSinh_Click">
                                    <StackPanel Orientation="Horizontal">
                                        <materialDesign:PackIcon Kind="Plus" VerticalAlignment="Center" Width="13" Height="13" Margin="0 1 8 0"/>
                                        <TextBlock Text="Thêm"/>

                                    </StackPanel>
                                </Button>
                                <Button x:Name="btn_Xoa_KhaiSinh" Style="{StaticResource MaterialDesignFlatAccentButton}" Grid.Row="1" materialDesign:ButtonAssist.CornerRadius="7" materialDesign:RippleAssist.Feedback="Salmon"
                                    Foreground="{DynamicResource PrimaryHueDarkBrush}" Width="100" Height="35" HorizontalAlignment="Right" Margin="0 10 0 0" FontSize="13" Click="btn_Xoa_KhaiSinh_Click">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
