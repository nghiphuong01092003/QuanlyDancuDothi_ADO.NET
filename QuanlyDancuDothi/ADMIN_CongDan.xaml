                                         materialDesign:HintAssist.Hint="Nơi đăng kí" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" />
                                <DatePicker x:Name="dtp_lyhon_ngaydk" Style="{StaticResource MaterialDesignOutlinedDatePicker}" Margin="5 0 5 10" materialDesign:HintAssist.Hint="Ngày đăng kí" Width="150"
                                            BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" Background="Transparent" >
                                </DatePicker>
                                <TextBox x:Name="txt_lyhon_lydo" Style="{StaticResource MaterialDesignOutlinedTextBox}" Height="auto" Width="184" Background="Transparent" Margin="5 0 5 10"
                                         materialDesign:HintAssist.Hint="Lí do" BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" FontSize="10" FontWeight="SemiBold" />
                                <TextBlock Text="Nhập lại" Margin="20 0 0 0" Height="24"/>
                                <Button x:Name="btn_clear_lyhon" Style="{StaticResource MaterialDesignFlatButton}" Width="auto" Margin="5 5 5 10" Click="btn_clear_lyhon_Click" >
                                    <materialDesign:PackIcon Kind="Delete"/>
                                </Button>
                            </WrapPanel>
                            <DataGrid CellStyle="{DynamicResource DataGridCellStyle1}" RowStyle="{DynamicResource DataGridRowStyle1}" Grid.Row="1" x:Name="dtg_LyHon" AutoGenerateColumns="False" CanUserAddRows="False" CanUserReorderColumns="False" GridLinesVisibility="None" Loaded="dtg_LyHon_Loaded" SelectionChanged="dtg_LyHon_SelectionChanged" >
                                <DataGrid.Columns>
                                    <DataGridTextColumn Binding="{Binding Malyhon}" Width="auto" Header="Mã đơn ly hôn" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding Makethon}" Width="auto" Header="Mã giấy chứng nhận kết hôn" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding CccdNguoiNopDon}" Width="auto" Header="CCCD người nộp đơn" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding Hotennguoinopdon}" Width="auto" Header="Họ tên người nộp đơn" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding CCCDVo}" Width="auto" Header="CCCD vợ" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding HotenVo}" Width="auto" Header="Họ và Tên vợ" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding CCCDChong}" Width="auto" Header="CCCD chồng" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding HotenChong}" Width="auto" Header="Họ và Tên chồng" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding Noidangky}" Width="auto" Header="Nơi đăng kí" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding Ngaydangky,StringFormat=d}" Width="auto" Header="Ngày đăng kí" IsReadOnly="True"/>
                                    <DataGridTextColumn Binding="{Binding Lydo}" Width="auto" Header="Lý do" IsReadOnly="True"/>
                                </DataGrid.Columns>
                            </DataGrid>
                            <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Left" >
                                <Button x:Name="btn_Them_Lyhon" Style="{StaticResource MaterialDesignFlatAccentButton}" Grid.Row="1" materialDesign:ButtonAssist.CornerRadius="7" materialDesign:RippleAssist.Feedback="Salmon"
                                    Foreground="{DynamicResource PrimaryHueDarkBrush}" Width="100" Height="35" HorizontalAlignment="Right" Margin="0 10 20 0" FontSize="13" Click="btn_Them_Lyhon_Click">
                                    <StackPanel Orientation="Horizontal">
                                        <materialDesign:PackIcon Kind="Plus" VerticalAlignment="Center" Width="13" Height="13" Margin="0 1 8 0"/>
                                        <TextBlock Text="Thêm"/>
                                    </StackPanel>
                                </Button>
                                <Button x:Name="btn_Xoa_Lyhon" Style="{StaticResource MaterialDesignFlatAccentButton}" Grid.Row="1" materialDesign:ButtonAssist.CornerRadius="7" materialDesign:RippleAssist.Feedback="Salmon"
                                    Foreground="{DynamicResource PrimaryHueDarkBrush}" Width="100" Height="35" HorizontalAlignment="Right" Margin="0 10 0 0" FontSize="13" Click="btn_Xoa_Lyhon_Click">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <materialDesign:PackIcon Kind="Delete" VerticalAlignment="Center" Margin="0 0 5 0"/>
                                        <TextBlock Text="Xóa" VerticalAlignment="Center" FontSize="13" FontWeight="SemiBold"/>
                                    </StackPanel>
                                </Button>
                                <Button x:Name="btn_Sua_Lyhon" Style="{StaticResource MaterialDesignFlatAccentButton}" Grid.Row="1" materialDesign:ButtonAssist.CornerRadius="7" materialDesign:RippleAssist.Feedback="Salmon"
                                    Foreground="{DynamicResource PrimaryHueDarkBrush}" Width="100" Height="35" HorizontalAlignment="Right" Margin="0 10 20 0" FontSize="13" Click="btn_Sua_Lyhon_Click">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                        <materialDesign:PackIcon Kind="AutoFix" VerticalAlignment="Center" Margin="0 0 5 0"/>
                                        <TextBlock Text="Sửa" VerticalAlignment="Center" FontSize="13" FontWeight="SemiBold"/>
                                    </StackPanel>
                                </Button>
                            </StackPanel>
                        </Grid>
                    </TabItem>
                </TabControl>
            </Grid>
        </Grid>
    </materialDesign:Card>
</Window>
